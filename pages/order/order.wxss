page {
  background: #f5f5f5;
}
.current{
  width: 90%;
  margin:10px auto 0;
  background-color: #fff;
  border-radius: 10px;
  padding: 10px;
}
.parentLevel::after{
  content: "";
  display: block;
  clear: both;
}
.left{
  float: left;
}
.right{
  float: right;
}

.col{
  color: #727272;
}
.topNav {
  width: 100%;
  height: 50px;
  line-height: 50px;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
}

.topNav view {
  width: 33.33%;
  float: left;
  text-align: center;
  border-bottom: 1px solid #f5f5f5;
}

view.con {
  color: #ff6422;
  border-bottom: 1px solid #ff6422;
}

swiper{
  margin-top: 51px;
}

.goodItem .up image{
  width: 80px;
  height: 80px;
}

.goodItem .up>view view{
  line-height: 30px;
  margin-left: 5px;
}

.goodItem .up>view .text{
  font-size: 12px;
  background: #fdf0d4;
  color: #ff6422;
  line-height: 20px;
  width: 88px;
  border-radius: 10px;
}
.goodItem .totalPrice{
  text-align: right;
  line-height: 40px;
}
.down{
  padding: 1px 0;
}
.down .btn{
  color:#ff6422;
  border:1px solid #ff6422;
  padding:5px 15px;
  border-radius: 50px;
}

/* 购物车为空 */
.null {
  text-align: center;
  font-size: 48rpx;
}

.null image {
  width: 50%;
  margin: 60rpx;
}

.null .toIndex {
  color: #fff;
  margin: 50rpx auto;
  background-color: #eb4450;
  width: 70%;
  padding: 20rpx 0;
}